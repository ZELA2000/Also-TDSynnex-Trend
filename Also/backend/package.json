{
    "name": "also-marketplace-backend",
    "version": "1.0.0",
    "description": "Node.js backend for Also Marketplace API integration",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "test:integration": "jest --testPathPattern=integration",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix"
    },
    "keywords": [
        "also",
        "marketplace",
        "api",
        "backend"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "express": "^4.18.2",
        "axios": "^1.6.0",
        "dotenv": "^16.3.1",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17",
        "@types/node": "^20.10.0",
        "@types/jest": "^29.5.10",
        "@typescript-eslint/eslint-plugin": "^6.13.0",
        "@typescript-eslint/parser": "^6.13.0",
        "eslint": "^8.54.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.2"
    }
}